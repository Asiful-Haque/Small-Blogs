CREATE USER 'remote_user'@'157.230.215.39' IDENTIFIED BY 'your_password';
GRANT SELECT, INSERT, UPDATE, DELETE ON your_database.* TO 'remote_user'@'157.230.215.39';
FLUSH PRIVILEGES;



